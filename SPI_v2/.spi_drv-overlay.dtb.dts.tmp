# 1 "<built-in>"
# 1 "<command-line>"
# 1 "/home/stud/projekt3/SPI_communication/SPI_v2/spi_drv-overlay.dts"
/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2835", "brcm,bcm2708";

    fragment@0 {
        target = <&spi0>;
        __overlay__ {
            status = "okay";
            spidev@0{
                status = "disabled";
            };
        };
    };

    fragment@1 {
        target = <&spi0>;
        __overlay__ {

            #address-cells = <1>;
            #size-cells = <0>;
            spi_drv:spi_drv@0 {
                compatible = "ase, spi_drv";
                reg = <0>;
                spi-cpha = <0>;
                spi-cpol = <0>;
                spi-max-frequency = <100000>;
            };
        };
    };
};
